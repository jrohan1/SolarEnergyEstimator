{
  "expo": {
    "name": "Solar Energy Estimator",
    "slug": "SEE",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.jpg",
    "splash": {
      "image": "./assets/logo.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#4160A1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    }
  },
  "infoPlist": {
    "NSCameraUsageDescription": "This app uses the camera to scan barcodes on event tickets.",
    "NSPhotoLibraryUsageDescription":"Solar Energy Estimator would like access to your camera roll"
  }
}
