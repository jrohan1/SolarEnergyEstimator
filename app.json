{
  "expo": {
    "name": "Solar Energy Estimator",
    "slug": "SEE",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.jpg",
    "splash": {
      "image": "./assets/logo.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#4160A1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.solarenergyestimator.see",
      "config": {
        "googleMapsApiKey": "API-key"
      },
      "infoPlist": {
        "NSCameraUsageDescription": "Solar Energy Estimator uses the camera to determine your roof pitch."
      }
    },
    "android": {
      "package": "com.solarenergyestimator.see",
      "config": {
        "googleSignIn": {
          "apiKey": "API-Key"
        },
        "googleMaps": {
          "apiKey": "API-KEY"
        }
      }
    },
    "description": ""
  }
}
